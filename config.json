{
    "service": {
        "name": "Asset Management Service",
        "run_as_daemon": true,
        "scan_interval": "5m",
        "auto_start": true
    },
    "network": {
        "interface": "auto",
        "auto_detect_local": true,
        "default_cidr": "192.168.123.0/24",
        "scan_local_network": true,
        "scan_file_list": true
    },
    "arp": {
        "enabled": true,
        "timeout": "2s",
        "workers": 200,
        "rate_limit": "50ms"
    },
    "netbios": {
        "enabled": true,
        "timeout": "3s",
        "workers": 20
    },
    "mdns": {
        "enabled": true,
        "timeout": "12s",
        "retries": 3,
        "concurrency": 10
    },
    "port_scan": {
        "enabled": true,
        "timeout": "2s",
        "workers": 20
    },
    "files": {
        "ip_list_file": "list.txt",
        "output_file": "assets.json",
        "settings_file": "settings.json"
    },
    "credentials": {
        "enabled": true,
        "settings_file": "settings.json",
        "timeout": "10s",
        "workers": 5,
        "test_default_creds": true
    },
    "nuclei": {
        "enabled": true,
        "severity": ["critical", "high", "medium"],
        "rate_limit": 10,
        "concurrency": 25,
        "timeout": "30s"
    }
}
